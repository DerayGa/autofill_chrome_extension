{
  "manifest_version": 2,
  "name": "Autofill ebank info",
  "description": "This extension will help me autofill login info.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_popup": "options.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["*://ebank.taipeifubon.com.tw/*", "*://ebank.esunbank.com.tw/*",
      "*://www.chb.com.tw/*", "*://ebank.yuantabank.com.tw/*", "*://mma.sinopac.com/*"],
      "js": ["jquery.min.js", "autofill.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}